{
  "name": "pdf-creation-actions-web-component",
  "version": "1.13.0",
  "description": "components actions to print or render the report to a pdf",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "lint": "eslint index.js",
    "release": "semantic-release",
    "demo:web": "http-server -p 8082",
    "test": "web-test-runner test/actionspdf.test.html --node-resolve --puppeteer",
    "start:watch": "web-dev-server --open demo/ --node-resolve --watch",
    "test:watch": "web-test-runner test/**/*.test.html --node-resolve --watch"
  },
  "repository": "https://github.com/TIGNUM/pdf-generation-actions-web-component.git",
  "author": "tignum",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TIGNUM/pdf-generation-actions-web-component/issues"
  },
  "homepage": "https://github.com/TIGNUM/pdf-generation-actions-web-component#readme",
  "dependencies": {
    "html2pdf.js": "0.9.3"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ],
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/preset-env": "^7.14.4",
    "@esm-bundle/chai": "^4.3.4",
    "@open-wc/testing": "^2.5.33",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@web/dev-server": "^0.1.17",
    "@web/test-runner": "^0.13.5",
    "@web/test-runner-mocha": "^0.7.2",
    "@web/test-runner-puppeteer": "^0.10.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "7.27.0",
    "eslint-plugin-sonarjs": "0.7.0",
    "http-server": "^0.12.3",
    "lit": "^2.0.0-rc.2",
    "lit-element": "^2.5.1",
    "lit-html": "^1.4.1",
    "puppeteer": "^10.0.0",
    "semantic-release": "^17.4.3",
    "webpack": "5.38.1",
    "webpack-cli": "4.7.0"
  }
}
